  "description": "Simple put.io Chrome extension",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jmugliston/simple-putio.git"
  },
  "keywords": [
    "simple-putio"
  ],
  "author": "James Mugliston",
  "license": "ISC",
  "engines": {
    "npm": ">=8.3.0"
  },
  "bugs": {
    "url": "https://github.com/jmugliston/simple-putio/issues"
  },
  "homepage": "https://github.com/jmugliston/simple-putio#readme",
  "scripts": {
    "format": "prettier --write src/*",
    "dev": "vite",
    "test": "vitest",
