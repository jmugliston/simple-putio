    "@putdotio/api-client": "^8.49.0",
    "axios": "^1.7.9",
    "copy-to-clipboard": "^3.3.3",
    "pretty-checkbox": "^3.0.3",
    "pretty-checkbox-react": "^3.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-modal": "^3.16.3",
    "react-timeago": "^7.2.0",
    "react-toastify": "^10.0.6",
    "react-tooltip": "^5.28.0"
